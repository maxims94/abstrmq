{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default" : {
      "format": "%(asctime)s:%(name)s:%(levelname)s:%(message)s"
    }
  },
  "handlers" : {
    "default": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "stream": "ext://sys.stderr",
      "formatter": "default"
    }
  },
  "loggers": {
    "st_base": {
       "level": "DEBUG",
       "handlers": ["default"]
    },
    "abstrmq": {
       "level": "DEBUG",
       "handlers": ["default"]
    },
    "aiormq": {
       "level": "DEBUG",
       "handlers": ["default"]
    },
    "asyncio": {
       "level": "DEBUG",
       "handlers": ["default"]
    }
  } 
}
